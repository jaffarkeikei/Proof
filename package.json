{
  "name": "proof",
  "version": "1.0.0",
  "description": "Autonomous Social Proof Engine - Transforms customer reviews into high-converting video testimonials",
  "main": "src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/server.js",
    "dev:frontend": "cd app && next dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build complete'",
    "build:frontend": "cd app && next build",
    "start": "node src/server.js",
    "start:frontend": "cd app && next start",
    "lint": "eslint src/ app/",
    "test": "jest"
  },
  "keywords": [
    "ai",
    "testimonials",
    "video",
    "automation",
    "social-proof"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "axios-retry": "^4.0.0",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "next": "^15.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "twilio": "^4.20.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
